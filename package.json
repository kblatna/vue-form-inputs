{
  "name": "@tvaliasek/vue-form-inputs",
  "version": "3.2.2",
  "private": false,
  "files": [
    "src",
    "dist",
    "!src/stories",
    "!src/components/__tests__",
    "!src/main.ts",
    "!src/App.vue",
    "!dist/index.html",
    "!dist/favicon.ico"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-form-inputs.mjs",
      "require": "./dist/vue-form-inputs.umd.js"
    },
    "./dist/vue-form-inputs.css": {
      "import": "./dist/vue-form-inputs.css",
      "require": "./dist/vue-form-inputs.css"
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "dist/vue-form-inputs.umd.js",
  "module": "dist/vue-form-inputs.mjs",
  "homepage": "https://github.com/tvaliasek/vue-form-inputs#readme",
  "bugs": {
    "url": "https://github.com/tvaliasek/vue-form-inputs/issues"
  },
  "keywords": [
    "form",
    "form input",
    "bootstrap",
    "vue"
  ],
  "license": "MIT",
  "author": {
    "name": "Tomáš Valiašek",
    "url": "https://github.com/tvaliasek"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run declarations:generate && npm run declarations:move",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts --fix --ignore-path .gitignore",
    "storybook": "cross-env NODE_OPTIONS=--openssl_legacy_provider start-storybook -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl_legacy_provider build-storybook",
    "declarations:generate": "vue-tsc --emitDeclarationOnly -p tsconfig.declarations.json",
    "declarations:move": "node ./.bin/moveDeclarations.js"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "@vuepic/vue-datepicker": "^4.2.0",
    "bootstrap-vue-next": "^0.7.3"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-vue-next": "^0.7.3",
    "unplugin-vue-components": "^0.24.0",
    "@vuepic/vue-datepicker": "^4.2.0",
    "@babel/core": "^7.20.12",
    "@iconify-json/bi": "^1.1.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.16",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-standard-with-typescript": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "sass": "^1.57.1",
    "typescript": "~4.7.4",
    "unplugin-icons": "^0.15.1",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-i18n": "^9.2.2",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  }
}
